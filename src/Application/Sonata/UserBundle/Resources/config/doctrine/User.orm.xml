<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                  http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Application\Sonata\UserBundle\Entity\User" table="user">

        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>
        <one-to-many target-entity="NCFrontBundle\Entity\Participant" mapped-by="user" field="participants"></one-to-many>
        <many-to-one target-entity="NCFrontBundle\Entity\Rank" field="rank" inversed-by="users"></many-to-one>

    </entity>

</doctrine-mapping>
